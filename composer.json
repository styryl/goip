{
    "name": "pikart/goip",
    "description": "Goip server and client",
    "keywords": ["sms","goip","gateway","client","server"],
    "license": "MIT",
    "authors": [
        {
            "name": "MichaÅ‚ Styrylski",
            "email": "styryl88@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "react/datagram": "^1.5",
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Pikart\\Goip\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12.88",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan-strict-rules": "^0.12.9",
        "phpmd/phpmd": "^2.10"
    },
    "autoload-dev": {
        "psr-4": {
            "Pikart\\Goip\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "runCodeAnalyze": [
            "vendor/bin/phpstan",
            "vendor/bin/phpmd src ansi ruleset.xml",
            "vendor/bin/phpcs"
        ],
        "runAllUnitTests": "vendor/bin/phpunit --testdox"
    }
}
